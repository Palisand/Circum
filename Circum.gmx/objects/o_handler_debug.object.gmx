          <argument>
            <kind>1</kind>
            <string>/// Guides

// Objects
var debug_text = "obj_count: "+string(obj_count)+"#";

// FPS
debug_text += ("fps: "+string(fps)+"#");
debug_text += ("fps_real: "+string(fps_real)+"#");

//orb type
var t = "DEFAULT";
if (orb_type == DEAD_ORB) { t = "DEAD"; }
else if (orb_type == VOID_ORB) { t = "VOID"; }
else if (orb_type == MASTER_ORB) { t = "MASTER"; }
debug_text += ("orb_type: "+t+"#");

//number of players
debug_text += ("num_players: "+string(instance_number(o_player_debug))+"#");

//if there is a player, show the number of orbs captured
if (instance_number(o_player_debug)) {
    var p = instance_find(o_player_debug,0);
    debug_text += ("player0 captured "+string(p.num_orb_captured)+"#");
}

draw_text_transformed(room_width/2, room_height/2, debug_text, 2,2,0);

if (mouse_check_button(mb_left)) {
    draw_set_colour(c_white);
    orbit_radius_draw = lerp(orbit_radius_draw, 80, 0.1);
    draw_circle(x_start, y_start, orbit_radius_draw, true);
    if (!keyboard_check(vk_shift)) {
        draw_arrow(x_start, y_start, mouse_x, mouse_y, 20);
    }
} 
else {
    orbit_radius_draw = 0;
}

if (mouse_check_button(mb_right)) {
    draw_set_colour(player_color_set);
    player_radius_draw = lerp(player_radius_draw, 5, 0.1);
    draw_circle(x_start, y_start, player_radius_draw, true);
    var dir = point_direction(x_start, y_start, mouse_x, mouse_y);
    draw_arrow(x_start, y_start, x_start + lengthdir_x(100, dir), y_start + lengthdir_y(100, dir), 20);
}
else {
    player_radius_draw = 0;
}

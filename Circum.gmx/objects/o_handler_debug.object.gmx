        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create Objects

// Set start coordinates
if (mouse_check_button(mb_left) || mouse_check_button(mb_right)) {
    if (!start_coords_set) {
        x_start = mouse_x;
        y_start = mouse_y;
        start_coords_set = true;
    }
}
// Prepare Orb
if (mouse_check_button(mb_left)) {
    orb_speed_set = point_distance(x_start, y_start, mouse_x, mouse_y) / 100;
    orb_dir_set = point_direction(x_start, y_start, mouse_x, mouse_y);
    // Use Mouse Wheel to set type
    if (mouse_wheel_up()) {
        orb_type = (orb_type + 1) % NUM_ORB_TYPES;
    }
    if (mouse_wheel_down() &amp;&amp; orb_type &gt; 0) {
        orb_type = (orb_type - 1) % NUM_ORB_TYPES;
    }
}
// Prepare Player
if (mouse_check_button(mb_right)) {
    player_dir_set = point_direction(x_start, y_start, mouse_x, mouse_y);
    if (keyboard_check(vk_control)) {
        // Use Mouse Wheel to set capture streak counter
        if (mouse_wheel_up()) {
            player_capture_streak++;
        }
        if (mouse_wheel_down() &amp;&amp; player_capture_streak &gt; 0) {
            player_capture_streak--;
        }
    }
    else if (keyboard_check(vk_tab)) {
        // Use Mouse Wheel to set ricochet streak counter
        if (mouse_wheel_up()) {
            player_ricochet_streak++;
        }
        if (mouse_wheel_down() &amp;&amp; player_ricochet_streak &gt; 0) {
            player_ricochet_streak--;
        }
    }
    else {
        // Use Mouse Wheel to set color (for identification purposes only)
        if (mouse_wheel_up()) {
            if (player_colors_index &lt; player_colors_len - 1) {
                player_colors_index++;
            }
            else {
                player_colors_index = 0;
            }
        }
        if (mouse_wheel_down()) {
            if (player_colors_index &gt; 0) {
                player_colors_index--;
            }
            else {
                player_colors_index = player_colors_len - 1;
            }
        }
    }
    player_color_set = player_colors[player_colors_index];
}
// Create Orb
if (mouse_check_button_released(mb_left)) {
    start_coords_set = false;
    if (keyboard_check(vk_tab)) {  // Shift: orb speed set to 0
        orb_speed_set = 0;
    }
    with (instance_create(x_start, y_start, o_orb_debug)) {
        speed = other.orb_speed_set;
        initial_speed = speed;
        direction = other.orb_dir_set;
        other.objects[other.obj_count++] = id;
        set_orb_type(self, other.orb_type);
    }
}
// Create Player
if (mouse_check_button_released(mb_right)) {
    start_coords_set = false;
    with (instance_create(x_start, y_start, o_player_debug)) {
        capture_streak = other.player_capture_streak;
        ricochet_streak = other.player_ricochet_streak;
        direction = other.player_dir_set;
        color = other.player_color_set;
        other.objects[other.obj_count++] = id;
    }
    player_capture_streak = 0;
    player_ricochet_streak = 0;
}
</string>
